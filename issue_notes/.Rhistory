library(np)
library(MASS)
data(birthwt)
birthwt$low <- factor(birthwt$low)
library(np)
nmulti <- 1
bw1 <- npcdensbw(low ~ lwt,
                 bwmethod = "cv.ls",
                 data = birthwt,
                 nmulti = nmulti)
bw2 <- npcdensbw(low ~ lwt,
                 bwmethod = "cv.ml",
                 data = birthwt,
                 nmulti = nmulti)
